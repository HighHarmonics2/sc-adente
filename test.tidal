d1 $ sound "bd <arpy:1 arpy:2 arpy:3>"
d1 $ sound "bd/2"
d1 $ sound "bd*3"
d1 $ sound "[bd sn]*2 cp"
d1 $ sound "[bd sn] cp/2"
d1 $ sound "[[bd sn] cp]*2"
d1 $ sound "[bd sn sn*3]/2 [bd sn*3 bd*4]/3"
d1 $ rev (sound "bd*2 [bd [sn sn*2 sn] sn]")
d1 $ every 4 (rev) (sound "bd*2 [bd [sn sn*2 sn] sn]")
d1 $ slow 4 $ sound "bd*2 [bd [sn sn*2 sn] sn]"
d1 $ every 4 (fast 2) (sound "bd*2 [bd [sn sn*2 sn] sn]")
d1 $ sound (every 4 (fast 4) "bd*2 [bd [sn sn*2 sn] sn]")
d1 $ sound "bd*4" # crush "6" # speed "2"
d1 $ sound "bd*4" # gain "1 0.8 0.5 0.7" # crush "4 4 6 7"
d1 $ sound "bd*4 sn*4" # gain "[[1 0.8]*2 [0.5 0.7]]/2"
d1 $ sound "bd*4" # gain (every 3 (rev) $ "1 0.8 0.5 0.7")
d1 $ gain "1 0.8 0.5 0.7" # sound "bd sd"
d1 $ every 2 (|> speed "2") $ sound "arpy*4" |> speed "1"
d1 $ every 4 (|* speed "1.5") $ sound "arpy*4" |> speed "1"
d1 $ every 3 (|+ pan "-1") $ every 2 (|+ gain "2") $ sound "arpy*4" |> note "0 2 4 5"
d1 $ sound "arpy(3,8)" # n "2 5"
d1 $ sound "arpy(3,8)" # n ("0 2" * 2)
d1 $ sound "arpy(3,8)" # n (every 4 (* 2) "0 2")
d1 $ n (off 0.125 (+12) $ off 0.25 (+7) $ slow 2 $ "0(3,8) [5 7]") # sound "arpy"
d1 $ n "[0 .. 7] [3 .. 1]" # sound "arpy"
d1 $ speed "-0.2 -5 1 0.5" # sound "arpy"

d1 silence

d2 $ sound "hh hh hh"
