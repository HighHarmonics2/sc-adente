all $ (# verb 0.8 0.7 "t" 0.7).(# tanh 1.5)

all $ id

do
  bpm 111
  d1
    $ while "<f!10 t!3>" ((chew 6 8))
    $ while "<f!3 t>" (iter 4)
    $ chunk 4 ((# pan rand).(|+ note 1.23))
    $ si (hurry "q")
    $ every 4 (rev)
    $ off "e" (
      (|* sp 2).(plyWith 4 (|+ n "0.2"))
      .(# bs (slow 12 $ tri))
      .(# pan (slow 8 $ tri))
      .(# lp (range 100 1000 $ slow 18 $ cosine) 0.15)
    )
    $ off "q" ((hurry 2).(# hp 64 0.2))
    $ n "0 [2.3 [4.4|6]]"
    # s "sin"
    |* g 0.9
    # boo

hush

