    $ stutWith 3 "s" ((|* g 0.95).(|* sp "[q|h]"))
    $ sb (slow 8 sine) ((|* legato 2).(slow 2))
    $ s "sin:0*12"
    # oc 7
    # legato 0.1
    # pan (slow 2 sine)
    |* g 0.8
    -- # rvb 0.5 0.7
    # lp 1200 0.15

