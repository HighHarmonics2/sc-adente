do
  bpm 111
  d1
    $ while "<f!10 t!3>" ((chew 6 8))
    $ while "<f!3 t>" (iter 4)
    -- $ chunk 4 ((# dis 0.5).(# pan rand))
    $ si (hurry "q")
    $ every 4 (rev)
    $ off "e" ((|* sp 2).(plyWith 4 (|+ n "0.2")).(# binshift (slow 12 $ tri)).(# pan (slow 8 $ tri)))
    $ off "q" ((hurry 2).(# hp 64 0.2))
    $ n "0 [2.3 [4.4|6]]"
    # s "sin"
    # rvb 0.3 0.5
    # lp (range 100 1000 $ slow 18 $ cosine) 0.15
    |* g 0.9
    # boo

hush

do
  bpm 123
  d1
    $ while "<f!6 t!2>" ((db "t").(stutWith 3 "s" ((|* sp 1.1).(|* pan rand))))
    $ every 3 (rev)
    $ every 4 (iter 2)
    $ si ((hurry "q").(# hp 66 0.3))
    $ off "3s" ((|+ oc 3).(# pan rand).(degradeBy "h"))
    $ off "e" ((|+ oc 2).(# lpf 1000).(degradeBy 0.5))
    $ off "s" ((|+ oc 1).(# pan "[0 1 0 1]"))
    $ n "[2(3,8), 1(1,8,3), 8(7,16,2)]"
    # s "sin"
    # oc "[4|5|6]"
    |* g 0.6
    # rvb 0.1 0.1
    # del "s" "t" "t"

hush

do
  bpm 123
  all
    $ id
    -- $ (bite 3 8)
  d1
    $ every 5 (rev)
    $ almostNever (|+ n 3)
    $ si ((hurry 4).(|* g 0.6).(degradeBy 0.5).(# pan (slow 4 sine)).(ply "[1|2]"))
    $ si ((hurry "q").(stutWith 3 "s" (|+ oc 1)))
    $ off "3e" (|+ oc 1)
    $ off "q" (|+ oc 2)
    $ euclid 3 9
    $ fast 2
    -- $ shut
    $ rarely (slow 2)
    $ s "sin:0"
    |* g 0.9
    # oc 4
    # rvb 0.1 0.1
    # del "s" "t" "t"
  d2
    $ stutWith 3 "s" ((|* g 0.95).(|* sp "[q|h]"))
    $ sb (slow 8 sine) ((|* legato 2).(slow 2))
    $ s "sin:0*12"
    # oc 7
    # legato 0.1
    # pan (slow 2 sine)
    # triode 0.3
    |* g 0.9
    # rvb 0.5 0.7
    # lp 1200 0.15

hush

do
  bpm 128
  -- resetCycles
  d1
    $ si (
      (hurry 4)
      .(slow 2)
      .(|* g 0.78)
      .(|+ oc "<2 1 2!2 0>")
    )
    $ si ((hurry "h").(|* g 0.8))
    $ juxBy (slow 8 $ ((saw*2)-1)) (slow "h")
    $ n "0 5 36 24 3 18"
    # s "sin"
    # oc 3
    # rvb 0.8 0.8
    # boo
    -- # dis (trigger 1 $ range 0 3 $ slow 128 $ envL)
    -- |* g (trigger 1 $ range 0 1 $ slow 16 $ envL)

hush


