do
  bpm 109
  d1
    $ chew "<1!8 2 3 4>/2" 4 -- play with this!
    $ euclid 2 3
    $ rarely (|- note "4")
    $ chunk 3 (off "q" (|+ note "[3|5]"))
    $ layer [
      id
      ,
      ("e" ~>).(hurry 3).(+| note "[1 2 3]").(sb "t" (plyWith 3 (|- note 12)))
