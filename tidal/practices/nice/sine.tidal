      ("h" ~>).(|- oc 2).(#ac "-4").(fast 2).(# boo).(# tanh 2.4)
      ,
      (every 3 (shut)).(# pan (choose [0,1])).(hurry 12).("h" ~>)
      .(rarely (|- note "5")).(fast 2).(#o 8).(#del "h" "t" "t")
      .(|* g 0.75)
    ]
    $ s "sin:0"
    # oc 5
    # legato 3
    # rvb 0.2 0.3

