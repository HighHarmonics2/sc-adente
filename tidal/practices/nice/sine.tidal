do
  bpm 98
  d1
    $ layer [
      (|- oc 2)
      .(|- n 3)
      .(# ac "-0.8")
      .(every 5 (iter 4))
      .(euclid 7 16)
      .(# legato 4)
      .(# shape 0.3)
      .(# tanh 2)
      ,
      (hurry 4)
      .(stutWith 4 "q" ((|* g 0.95).(|+ n (choose [0,-3,3,5,-5])).(# pan rand)))
      .(# lpf 600)
      .(# triode 0.8)
      ,
      ("[q|s]" ~>)
      .(hurry 8)
      .(rarely (fast 2))
      .(# o 8)
      .(#rvb 0.1 0.1)
      .(# dis 0.38)
      ,
      (|* sp 4)
      -- .(db 0.3)
      .(# ac "5")
      .(slow 3)
      .(euclid 5 6)
      .(stutWith 4 "0.1" ((|* g 0.95).(|* sp (rand+0.5))))
      .(# lp 800 0.2)
      .(# legato 0.4)
    ]
    $ s "sin:0"
    # oc 5


hush

do
  bpm 100
  d1
    $ chunk 5 ((off "h" ((slow 2).(|* sp 1.5))).(# pan 0.1))
    $ chunk 3 ((off "h" (hurry 2)).(# pan 0.9))
    $ layer [
      id
      .(sometimesBy "x" ((|+ dis 0.3).(|* g 0.7)))
      .(sometimesBy "t" (+| n (segment 2 $ scale "minor" (irand 8))))
      ,
      (euclid 7 8)
      .(|- oc 2)
      .(# ac "-0.4")
      .(# shape 0.33)
      .(# rvb 0.3 0.4)
      .(# o 7)
      ,
      (slow 4)
      .(db "q")
      .(sb "h" (off "h" ((|- oc 2).(|* tremolo 2 1.5))).(# pan rand))
      .(|+ n (choose [0,3,5]))
      .(|+ dis 0.6)
      .(|+ oc 1)
      .(|* g 0.43)
      .(# ac ((range 0.5 1 $ rand)*(choose [-1,1])))
      .(# tremolo (irand 4 + 1) 0.5)
      .(|+ rvb 0.2 0.4)
      .(# o 8)
      ,
      (+| n "~!3 [3|3*2]")
      .(# lpf 800)
      .(|* g 0.8)
      ,
      (euclid 15 16)
      .(|* sp 12)
      .(# legato 2)
      .(|* g 0.6)
      .(# scram (slow 18 $ tri))
    ]
    $ s "sin:0"
    # oc 5

hush

