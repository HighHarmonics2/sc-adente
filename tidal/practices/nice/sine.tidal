do
  bpm 100
  d1
    $ chunk 5 ((off "h" ((slow 2).(|* sp 1.5))).(# pan 0.1))
    $ chunk 3 ((off "h" (hurry 2)).(# pan 0.9))
    $ layer [
      id
      .(sometimesBy "x" ((|+ dis 0.3).(|* g 0.7)))
      .(sometimesBy "t" (+| n (segment 2 $ scale "minor" (irand 8))))
      ,
      (euclid 7 8)
      .(|- oc 2)
      .(# hp 54 0.2)
      .(# ac "-0.4")
      .(# shape 0.2)
      -- .(# rvb 0.3 0.4)
      .(# o 7)
      ,
      (slow 4)
      .(db "q")
      .(sb "h" (off "h" ((|- oc 2).(|* tremolo 2 1.5))).(# pan rand))
      .(|+ n (choose [0,3,5]))
      .(|+ dis 0.3)
      .(|+ oc 1)
      .(|* g 0.43)
      .(# ac ((range 0.5 1 $ rand)*(choose [-1,1])))
      .(# tremolo (irand 4 + 1) 0.5)
      -- .(|+ rvb 0.2 0.4)
      .(# o 8)
      ,
      (+| n "~!3 [3|3*2]")
      .(# lpf 800)
      .(|* g 0.8)
      ,
      (euclid 15 16)
      .(|* sp 12)
      .(# legato 2)
      .(|* g 0.6)
      .(# scram (slow 18 $ tri))
    ]
    $ s "sin:0"
    # oc 5

hush

